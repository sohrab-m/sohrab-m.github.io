journal = {Neural Computing and Applications},
keywords = {Design abstraction,Distributed control,Hierarchical morphology and control,Modular self-reconfigurable robots,Scalability,Versatility},
number = {6},
pages = {787--805},
title = {{Anatomy-based organization of morphology and control in self-reconfigurable modular robots}},
volume = {19},
year = {2010}
}
